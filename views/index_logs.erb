<div class="col-md-12">
    <div class="row bbva">
      <div class="col-md-3 margin" >
      </div>
      <div class="col-md-6"><h1 class="text-center white" ><%= titulo %></h1></div>
      <div class="col-md-3">
        <%= partial 'partials/navegador', admin: admin %>
      </div>
    </div>
</div>
<div class="col-md-12">
  <table class="table table-bordered table-striped table-condensed text-left data-table-logs">
    <thead>
      <tr>
        <th class="col-md-2">Fecha</th>
        <th class="col-md-1">ID</th>
        <th class="col-md-1">Severidad</th>
        <th class="col-md-6">Mensaje</th>
        <th class="col-md-2">Usuario</th>
      </tr>
    </thead>

    <tbody>
      <% logs.each do |log| %>
        <tr>
          <td class="col-md-2"><%= log.created_at.to_s(:db) %></td>
          <td class="col-md-1"><%= log.id %></td>
          <td class="col-md-1"><%= log.severidad %></td>
          <td class="col-md-6"><%= log.mensaje %></td>
          <td class="col-md-2"><%= log.usuario && log.usuario.nombre %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
