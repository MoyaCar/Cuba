<div class="col-md-12">
  <p>&nbsp;</p>

  <form action='/admin-login' method='post' class="row form">
    <div class="col-sm-offset-4 col-sm-4 form-group">
      <input type="text" name="legajo" class="form-control input-lg enfocable" required autofocus
        data-target="input[name='legajo']"
        autocomplete="off"
      />
    </div>

    <div class="col-sm-offset-4 col-sm-4 form-group">
      <input type="password" name="codigo" class="form-control input-lg enfocable" required autofocus
        data-target="input[name='codigo']"
        autocomplete="new-password"
        />
    </div>

    <div class="col-sm-offset-4 col-sm-4 form-group teclado-contenedor"></div>

    <div class="col-sm-offset-4 col-sm-4 form-group">
      <button type="submit" class="btn btn-lg btn-success" data-disable-with="Enviando...">Siguiente</button>
    </div>
  </form>
</div>
